<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.043" />

    <ImageView
        android:id="@+id/backgroundimage1"
        android:layout_width="380dp"
        android:layout_height="303dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.219"
        app:srcCompat="@drawable/batteryinformationbackground" />

    <TextView
        android:id="@+id/maxvoltagetext"
        android:layout_width="136dp"
        android:layout_height="56dp"
        android:text="Max Voltage"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/minvoltagetext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.21"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/backgroundimage1" />

    <TextView
        android:id="@+id/minvoltagetext"
        android:layout_width="136dp"
        android:layout_height="56dp"
        android:text="Min Voltage"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/avgvoltagetext"
        app:layout_constraintEnd_toEndOf="@+id/backgroundimage1"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="@+id/backgroundimage1"
        app:layout_constraintTop_toTopOf="@+id/backgroundimage1"
        app:layout_constraintVertical_bias="0.769" />

    <TextView
        android:id="@+id/avgvoltagetext"
        android:layout_width="136dp"
        android:layout_height="56dp"
        android:layout_marginTop="208dp"
        android:text="Avg Voltage"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/backgroundimage1"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="@+id/backgroundimage1"
        app:layout_constraintTop_toTopOf="@+id/backgroundimage1" />

</androidx.constraintlayout.widget.ConstraintLayout>